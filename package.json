{
  "name": "eslint-config-vectron",
  "version": "2.2.0",
  "description": "Eslint config for my react projects",
  "main": "index.js",
  "author": "Aaron Lampros <alampros@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "eslint .",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alampros/eslint-config-vectron.git"
  },
  "keywords": [
    "eslint",
    "tools"
  ],
  "bugs": {
    "url": "https://github.com/alampros/eslint-config-vectron/issues"
  },
  "homepage": "https://github.com/alampros/eslint-config-vectron#readme",
  "peerDependencies": {
    "eslint": ">= 7",
    "typescript": ">= 4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^6.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "conventional-changelog-conventionalcommits": "^4.4.0",
    "eslint": "^7.15.0",
    "semantic-release": "^17.3.0",
    "typescript": "^4.1.2"
  },
  "eslintIgnore": [
    "!/.*",
    ".git"
  ],
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
