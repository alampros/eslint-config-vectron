{
  "name": "eslint-config-vectron",
  "version": "4.0.0-beta.1",
  "description": "Eslint config for my react projects",
  "main": "index.js",
  "author": "Aaron Lampros <alampros@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "eslint .",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alampros/eslint-config-vectron.git"
  },
  "keywords": [
    "eslint",
    "tools"
  ],
  "bugs": {
    "url": "https://github.com/alampros/eslint-config-vectron/issues"
  },
  "homepage": "https://github.com/alampros/eslint-config-vectron#readme",
  "peerDependencies": {
    "eslint": ">= 8",
    "typescript": ">= 4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "eslint": "^8.3.0",
    "semantic-release": "^18.0.1",
    "typescript": "^4.5.2"
  },
  "eslintIgnore": [
    "!/.*",
    ".git"
  ],
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "channel": "beta",
        "prerelease": "beta"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
